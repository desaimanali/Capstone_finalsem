{
    "manifest_version": 3,
    "name": "WEbSaFe Sentinel",
    "version": "1.0",
    "description": "A Chrome extension for website vulnerability scanning",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage", "scripting"],
    "host_permissions": ["<all_urls>"]
  }
  